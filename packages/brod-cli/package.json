{
    "name": "brod",
    "version": "0.0.45",
    "author": "mads hargreave @madshargreave",
    "bin": {
        "brod": "./bin/run"
    },
    "bugs": "https://github.com/madshargreave/brod/issues",
    "dependencies": {
        "@krans/project": "0.0.10",
        "@krans/runner": "^0.0.29",
        "@oclif/command": "^1.8.0",
        "@oclif/config": "^1.17.0",
        "@oclif/plugin-help": "^3.2.2",
        "tslib": "^2.3.0"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1.26.0",
        "@oclif/test": "^1.2.8",
        "@types/chai": "^4.2.19",
        "@types/mocha": "^8.2.2",
        "@types/node": "^15.12.5",
        "chai": "^4.3.4",
        "eslint-config-oclif": "^3.1.0",
        "eslint-config-oclif-typescript": "^0.2.0",
        "globby": "^11.0.4",
        "mocha": "^9.0.1",
        "nyc": "^15.1.0"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/madshargreave/brod",
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "dist/index.js",
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "eslint . --ext .ts --config .eslintrc",
        "prepack": "rm -rf dist && tsc -b && oclif-dev manifest && oclif-dev readme",
        "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif-dev readme && git add README.md",
        "clean": "rimraf dist",
        "build": "tsc",
        "watch": "tsc --watch"
    },
    "oclif": {
        "commands": "./dist/commands",
        "bin": "brod",
        "plugins": [
            "@oclif/plugin-help"
        ]
    },
    "repository": "madshargreave/brod",
    "publishConfig": {
        "registry": "http://registry.npmjs.org/",
        "access": "public"
    },
    "types": "dist/index.d.ts"
}
